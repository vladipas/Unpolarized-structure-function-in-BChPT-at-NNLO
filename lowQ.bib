@article{Ivanov:2011fk,
	Abstract = {High-energy photons and other particles carrying non-zero orbital angular momentum (OAM) emerge as a new tool in high-energy physics. Generation of energetic photons with non-zero OAM (twisted photons) in Compton backscattering seems feasible with today's technology, and collisions of high-energy twisted states can offer a new look at particle properties and interactions. A theoretical description of twisted particle scattering developed previously treated them as pure Bessel states. Here we develop further this formalism by incoprorating two additional important features. First, we treat the initial OAM state as a wave packet of a finite transverse size rather than a pure Bessel state. This realistic assumption allows us to resolve the existing controversy between two theoretical analyses for a non-backward scattering. Second, we describe the final twisted particle in terms of the OAM projection on its average direction of propagation (which we call the "orbital helicity") rather than the OAM projection on the fixed reaction axis. Using this formalism, we determine to what extent the twisted state is transferred from the initial to final OAM particle in a generic scattering kinematics.},
	Author = {I. P. Ivanov and V. G. Serbo},
	Date-Added = {2011-06-01 15:59:19 +0200},
	Date-Modified = {2011-06-01 15:59:19 +0200},
	Eprint = {1105.6244},
	Month = {05},
	Title = {Scattering of twisted particles: extension to wave packets},
	Url = {http://arxiv.org/abs/1105.6244},
	Year = {2011},
	Bdsk-Url-1 = {http://arxiv.org/abs/1105.6244}}

